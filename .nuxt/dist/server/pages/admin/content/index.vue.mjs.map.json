AAW,QACX,MAAO,QACP,SAAU,OACX,GACeA,CAAM,GAAKA,EAGvBY,EAAkBZ,IACP,CACb,UAAW,8BACX,MAAO,gCACP,SAAU,2BACX,GACaA,CAAM,GAAK,4BAI3B,OAAAa,GAAM,CAAClC,EAAaC,EAAcC,CAAU,EAAG,IAAM,CACnDC,EAAY,MAAQ,CACtB,CAAC,EAeDgC,GAAQ,CACN,MAAO,oCACP,KAAM,CACJ,CAAE,KAAM,cAAe,QAAS,6BAA+B,EAC/D,CAAE,KAAM,SAAU,QAAS,mBAAqB,CACjD,CACH,CAAC","names":["itemsPerPage","pages","ref","loading","searchQuery","statusFilter","typeFilter","currentPage","filteredPages","computed","filtered","page","totalItems","totalPages","totalPagesCount","publishedPages","draftPages","todayPages","today","contentStats","typeCounts","acc","type","getTypeName","statusCounts","status","getStatusName","monthlyCounts","months","month","monthIndex","formatDate","dateString","truncateText","text","length","getTypeColor","getStatusColor","watch","useHead"],"sources":["../../../../../../pages/admin/content/index.vue"],"sourcesContent":["<template>\r\n  <div class=\"space-y-6\">\r\n    <!-- ŸÖÿ≥ÿßÿ≠ÿ© ÿ•ÿ∂ÿßŸÅŸäÿ© ŸÅŸä ÿßŸÑÿ£ÿπŸÑŸâ -->\r\n    <div class=\"h-16\"></div>\r\n    \r\n    <!-- ÿ±ÿ£ÿ≥ ÿßŸÑÿµŸÅÿ≠ÿ© -->\r\n    <AdminPageHeader \r\n      title=\"ÿ•ÿØÿßÿ±ÿ© ÿßŸÑŸÖÿ≠ÿ™ŸàŸâ\"\r\n      description=\"ÿ•ÿØÿßÿ±ÿ© ÿµŸÅÿ≠ÿßÿ™ ÿßŸÑŸÖŸàŸÇÿπ ŸàÿßŸÑŸÖÿ≠ÿ™ŸàŸâ\"\r\n    >\r\n      <template #actions>\r\n        <NuxtLink\r\n          to=\"/admin/content/analytics\"\r\n          class=\"inline-flex items-center px-3 sm:px-4 py-2 bg-indigo-600 text-white rounded-lg hover:bg-indigo-700 transition-colors text-sm\"\r\n        >\r\n          <Icon name=\"material-symbols:analytics\" class=\"h-4 w-4 sm:h-5 sm:w-5 ml-2\" />\r\n          <span class=\"hidden sm:inline\">ÿßŸÑÿ•ÿ≠ÿµÿßÿ¶Ÿäÿßÿ™</span>\r\n          <span class=\"sm:hidden\">ÿ•ÿ≠ÿµÿßÿ¶Ÿäÿßÿ™</span>\r\n        </NuxtLink>\r\n        <NuxtLink\r\n          to=\"/admin/content/templates\"\r\n          class=\"inline-flex items-center px-3 sm:px-4 py-2 bg-green-600 text-white rounded-lg hover:bg-green-700 transition-colors text-sm\"\r\n        >\r\n          <Icon name=\"material-symbols:widgets\" class=\"h-4 w-4 sm:h-5 sm:w-5 ml-2\" />\r\n          <span class=\"hidden sm:inline\">ÿßŸÑŸÇŸàÿßŸÑÿ®</span>\r\n          <span class=\"sm:hidden\">ŸÇŸàÿßŸÑÿ®</span>\r\n        </NuxtLink>\r\n        <NuxtLink\r\n          to=\"/admin/content/editor\"\r\n          class=\"inline-flex items-center px-3 sm:px-4 py-2 bg-purple-600 text-white rounded-lg hover:bg-purple-700 transition-colors text-sm\"\r\n        >\r\n          <Icon name=\"material-symbols:edit\" class=\"h-4 w-4 sm:h-5 sm:w-5 ml-2\" />\r\n          <span class=\"hidden sm:inline\">ŸÖÿ≠ÿ±ÿ± ÿßŸÑŸÖÿ≠ÿ™ŸàŸâ</span>\r\n          <span class=\"sm:hidden\">ŸÖÿ≠ÿ±ÿ±</span>\r\n        </NuxtLink>\r\n        <NuxtLink\r\n          to=\"/admin/content/create\"\r\n          class=\"inline-flex items-center px-3 sm:px-4 py-2 bg-blue-600 text-white rounded-lg hover:bg-blue-700 transition-colors text-sm\"\r\n        >\r\n          <Icon name=\"material-symbols:add\" class=\"h-4 w-4 sm:h-5 sm:w-5 ml-2\" />\r\n          <span class=\"hidden sm:inline\">ÿ•ÿ∂ÿßŸÅÿ© ÿµŸÅÿ≠ÿ© ÿ¨ÿØŸäÿØÿ©</span>\r\n          <span class=\"sm:hidden\">ÿ•ÿ∂ÿßŸÅÿ©</span>\r\n        </NuxtLink>\r\n      </template>\r\n    </AdminPageHeader>\r\n\r\n    <!-- ÿ•ÿ≠ÿµÿßÿ¶Ÿäÿßÿ™ ÿ≥ÿ±Ÿäÿπÿ© -->\r\n    <AdminStats :stats=\"contentStats\" />\r\n\r\n\r\n\r\n\r\n\r\n\r\n    <!-- ÿ¥ÿ±Ÿäÿ∑ ÿßŸÑÿ®ÿ≠ÿ´ ŸàÿßŸÑÿ™ÿµŸÅŸäÿ© -->\r\n    <div class=\"bg-white rounded-lg shadow-sm border border-gray-200 p-6\">\r\n      <div class=\"grid grid-cols-1 md:grid-cols-3 gap-4\">\r\n        <!-- ÿßŸÑÿ®ÿ≠ÿ´ -->\r\n        <div class=\"relative\">\r\n          <Icon name=\"material-symbols:search\" class=\"absolute right-3 top-1/2 transform -translate-y-1/2 h-5 w-5 text-gray-400\" />\r\n          <input\r\n            v-model=\"searchQuery\"\r\n            type=\"text\"\r\n            placeholder=\"ÿßŸÑÿ®ÿ≠ÿ´ ŸÅŸä ÿßŸÑÿµŸÅÿ≠ÿßÿ™...\"\r\n            class=\"w-full pr-10 pl-4 py-2 border border-gray-300 rounded-lg focus:ring-2 focus:ring-blue-500 focus:border-transparent\"\r\n          />\r\n        </div>\r\n\r\n        <!-- ÿ™ÿµŸÅŸäÿ© ÿ≠ÿ≥ÿ® ÿßŸÑÿ≠ÿßŸÑÿ© -->\r\n        <select\r\n          v-model=\"statusFilter\"\r\n          class=\"w-full px-4 py-2 border border-gray-300 rounded-lg focus:ring-2 focus:ring-blue-500 focus:border-transparent\"\r\n        >\r\n          <option value=\"\">ÿ¨ŸÖŸäÿπ ÿßŸÑÿ≠ÿßŸÑÿßÿ™</option>\r\n          <option value=\"published\">ŸÖŸÜÿ¥Ÿàÿ±</option>\r\n          <option value=\"draft\">ŸÖÿ≥ŸàÿØÿ©</option>\r\n          <option value=\"archived\">ŸÖÿ§ÿ±ÿ¥ŸÅ</option>\r\n        </select>\r\n\r\n        <!-- ÿ™ÿµŸÅŸäÿ© ÿ≠ÿ≥ÿ® ÿßŸÑŸÜŸàÿπ -->\r\n        <select\r\n          v-model=\"typeFilter\"\r\n          class=\"w-full px-4 py-2 border border-gray-300 rounded-lg focus:ring-2 focus:ring-blue-500 focus:border-transparent\"\r\n        >\r\n          <option value=\"\">ÿ¨ŸÖŸäÿπ ÿßŸÑÿ£ŸÜŸàÿßÿπ</option>\r\n          <option value=\"page\">ÿµŸÅÿ≠ÿ©</option>\r\n          <option value=\"post\">ŸÖŸÇÿßŸÑ</option>\r\n          <option value=\"news\">ÿ£ÿÆÿ®ÿßÿ±</option>\r\n        </select>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- ŸÇÿßÿ¶ŸÖÿ© ÿßŸÑÿµŸÅÿ≠ÿßÿ™ -->\r\n    <div class=\"bg-white rounded-lg shadow-sm border border-gray-200\">\r\n      <!-- ÿ≠ÿßŸÑÿ© ÿßŸÑÿ™ÿ≠ŸÖŸäŸÑ -->\r\n      <div v-if=\"loading\" class=\"flex justify-center items-center py-12\">\r\n        <Icon name=\"material-symbols:progress-activity\" class=\"animate-spin h-8 w-8 text-blue-600\" />\r\n        <span class=\"mr-3 text-gray-600\">ÿ¨ÿßÿ±Ÿç ÿ™ÿ≠ŸÖŸäŸÑ ÿßŸÑÿµŸÅÿ≠ÿßÿ™...</span>\r\n      </div>\r\n\r\n      <!-- ŸÇÿßÿ¶ŸÖÿ© ÿßŸÑÿµŸÅÿ≠ÿßÿ™ -->\r\n      <div v-else-if=\"filteredPages.length > 0\" class=\"overflow-x-auto\">\r\n        <table class=\"min-w-full divide-y divide-gray-200\">\r\n          <thead class=\"bg-gray-50\">\r\n            <tr>\r\n              <th class=\"px-6 py-3 text-right text-xs font-medium text-gray-500 uppercase tracking-wider\">\r\n                ÿßŸÑÿµŸÅÿ≠ÿ©\r\n              </th>\r\n              <th class=\"px-6 py-3 text-right text-xs font-medium text-gray-500 uppercase tracking-wider\">\r\n                ÿßŸÑŸÜŸàÿπ\r\n              </th>\r\n              <th class=\"px-6 py-3 text-right text-xs font-medium text-gray-500 uppercase tracking-wider\">\r\n                ÿßŸÑÿ≠ÿßŸÑÿ©\r\n              </th>\r\n              <th class=\"px-6 py-3 text-right text-xs font-medium text-gray-500 uppercase tracking-wider\">\r\n                ÿ™ÿßÿ±ŸäÿÆ ÿßŸÑÿ•ŸÜÿ¥ÿßÿ°\r\n              </th>\r\n              <th class=\"px-6 py-3 text-right text-xs font-medium text-gray-500 uppercase tracking-wider\">\r\n                ÿßŸÑÿ•ÿ¨ÿ±ÿßÿ°ÿßÿ™\r\n              </th>\r\n            </tr>\r\n          </thead>\r\n          <tbody class=\"bg-white divide-y divide-gray-200\">\r\n            <tr v-for=\"page in filteredPages\" :key=\"page.id\" class=\"hover:bg-gray-50\">\r\n              <!-- ŸÖÿπŸÑŸàŸÖÿßÿ™ ÿßŸÑÿµŸÅÿ≠ÿ© -->\r\n              <td class=\"px-6 py-4 whitespace-nowrap\">\r\n                <div class=\"flex items-center\">\r\n                  <div class=\"flex-shrink-0 h-10 w-10\">\r\n                    <div class=\"h-10 w-10 rounded-lg bg-gray-300 flex items-center justify-center\">\r\n                      <Icon name=\"material-symbols:article\" class=\"h-6 w-6 text-gray-600\" />\r\n                    </div>\r\n                  </div>\r\n                  <div class=\"mr-4\">\r\n                    <div class=\"text-sm font-medium text-gray-900\">{{ page.title_ar || page.title_en || page.title }}</div>\r\n                    <div class=\"text-sm text-gray-500\">{{ truncateText(page.content_ar || page.content_en || page.content, 50) }}</div>\r\n                  </div>\r\n                </div>\r\n              </td>\r\n\r\n              <!-- ÿßŸÑŸÜŸàÿπ -->\r\n              <td class=\"px-6 py-4 whitespace-nowrap\">\r\n                <span :class=\"getTypeColor(page.type)\" class=\"px-2 py-1 text-xs font-medium rounded-full\">\r\n                  {{ getTypeName(page.type) }}\r\n                </span>\r\n              </td>\r\n\r\n              <!-- ÿßŸÑÿ≠ÿßŸÑÿ© -->\r\n              <td class=\"px-6 py-4 whitespace-nowrap\">\r\n                <span :class=\"getStatusColor(page.status)\" class=\"px-2 py-1 text-xs font-medium rounded-full\">\r\n                  {{ getStatusName(page.status) }}\r\n                </span>\r\n              </td>\r\n\r\n              <!-- ÿ™ÿßÿ±ŸäÿÆ ÿßŸÑÿ•ŸÜÿ¥ÿßÿ° -->\r\n              <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-500\">\r\n                {{ formatDate(page.created_at) }}\r\n              </td>\r\n\r\n              <!-- ÿßŸÑÿ•ÿ¨ÿ±ÿßÿ°ÿßÿ™ -->\r\n              <td class=\"px-6 py-4 whitespace-nowrap text-right text-sm font-medium\">\r\n                <div class=\"flex items-center space-x-2 space-x-reverse\">\r\n                  <NuxtLink\r\n                    :to=\"`/admin/content/preview/${page.id}`\"\r\n                    class=\"text-blue-600 hover:text-blue-900 p-1 rounded\"\r\n                    title=\"ŸÖÿπÿßŸäŸÜÿ©\"\r\n                  >\r\n                    <Icon name=\"material-symbols:visibility\" class=\"h-4 w-4\" />\r\n                  </NuxtLink>\r\n                  <NuxtLink\r\n                    :to=\"`/admin/content/editor?page=${page.id}`\"\r\n                    class=\"text-purple-600 hover:text-purple-900 p-1 rounded\"\r\n                    title=\"ÿ™ÿ≠ÿ±Ÿäÿ± ŸÖÿ™ŸÇÿØŸÖ\"\r\n                  >\r\n                    <Icon name=\"material-symbols:design-services\" class=\"h-4 w-4\" />\r\n                  </NuxtLink>\r\n                  <NuxtLink\r\n                    :to=\"`/admin/content/${page.id}/edit`\"\r\n                    class=\"text-green-600 hover:text-green-900 p-1 rounded\"\r\n                    title=\"ÿ™ÿ≠ÿ±Ÿäÿ±\"\r\n                  >\r\n                    <Icon name=\"material-symbols:edit\" class=\"h-4 w-4\" />\r\n                  </NuxtLink>\r\n                  <button\r\n                    @click=\"togglePageStatus(page)\"\r\n                    :class=\"page.status === 'published' ? 'text-orange-600 hover:text-orange-900' : 'text-green-600 hover:text-green-900'\"\r\n                    class=\"p-1 rounded\"\r\n                    :title=\"page.status === 'published' ? 'ÿ•ŸÑÿ∫ÿßÿ° ÿßŸÑŸÜÿ¥ÿ±' : 'ŸÜÿ¥ÿ±'\"\r\n                  >\r\n                    <Icon :name=\"page.status === 'published' ? 'material-symbols:visibility-off' : 'material-symbols:visibility'\" class=\"h-4 w-4\" />\r\n                  </button>\r\n                  <button\r\n                    @click=\"deletePage(page)\"\r\n                    class=\"text-red-600 hover:text-red-900 p-1 rounded\"\r\n                    title=\"ÿ≠ÿ∞ŸÅ\"\r\n                  >\r\n                    <Icon name=\"material-symbols:delete\" class=\"h-4 w-4\" />\r\n                  </button>\r\n                </div>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n\r\n      <!-- ÿ≠ÿßŸÑÿ© ÿπÿØŸÖ Ÿàÿ¨ŸàÿØ ÿ®ŸäÿßŸÜÿßÿ™ -->\r\n      <div v-else class=\"text-center py-12\">\r\n        <Icon name=\"material-symbols:article\" class=\"h-16 w-16 text-gray-300 mx-auto mb-4\" />\r\n        <h3 class=\"text-lg font-medium text-gray-900 mb-2\">ŸÑÿß ÿ™Ÿàÿ¨ÿØ ÿµŸÅÿ≠ÿßÿ™</h3>\r\n        <p class=\"text-gray-500 mb-6\">ŸÑŸÖ Ÿäÿ™ŸÖ ÿßŸÑÿπÿ´Ÿàÿ± ÿπŸÑŸâ ÿ£Ÿä ÿµŸÅÿ≠ÿßÿ™</p>\r\n        <NuxtLink\r\n          to=\"/admin/content/create\"\r\n          class=\"inline-flex items-center px-4 py-2 bg-blue-600 text-white rounded-lg hover:bg-blue-700 transition-colors\"\r\n        >\r\n          <Icon name=\"material-symbols:add\" class=\"h-5 w-5 ml-2\" />\r\n          ÿ•ÿ∂ÿßŸÅÿ© ÿµŸÅÿ≠ÿ© ÿ¨ÿØŸäÿØÿ©\r\n        </NuxtLink>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- ÿßŸÑÿ™ÿµŸÅÿ≠ -->\r\n    <div v-if=\"totalPages > 1\" class=\"flex items-center justify-between\">\r\n      <div class=\"text-sm text-gray-700\">\r\n        ÿπÿ±ÿ∂ {{ (currentPage - 1) * itemsPerPage + 1 }} ÿ•ŸÑŸâ {{ Math.min(currentPage * itemsPerPage, totalItems) }} ŸÖŸÜ ÿ£ÿµŸÑ {{ totalItems }} ÿµŸÅÿ≠ÿ©\r\n      </div>\r\n      <div class=\"flex space-x-1 space-x-reverse\">\r\n        <button\r\n          @click=\"currentPage = Math.max(1, currentPage - 1)\"\r\n          :disabled=\"currentPage === 1\"\r\n          class=\"px-3 py-2 text-sm bg-white border border-gray-300 rounded-md hover:bg-gray-50 disabled:opacity-50 disabled:cursor-not-allowed\"\r\n        >\r\n          ÿßŸÑÿ≥ÿßÿ®ŸÇ\r\n        </button>\r\n        <span class=\"px-3 py-2 text-sm bg-blue-600 text-white border border-blue-600 rounded-md\">\r\n          {{ currentPage }}\r\n        </span>\r\n        <button\r\n          @click=\"currentPage = Math.min(totalPages, currentPage + 1)\"\r\n          :disabled=\"currentPage === totalPages\"\r\n          class=\"px-3 py-2 text-sm bg-white border border-gray-300 rounded-md hover:bg-gray-50 disabled:opacity-50 disabled:cursor-not-allowed\"\r\n        >\r\n          ÿßŸÑÿ™ÿßŸÑŸä\r\n        </button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { PieChart, DoughnutChart, BarChart } from '~/components/charts'\r\n\r\n// ÿ•ÿπÿØÿßÿØ ÿßŸÑÿµŸÅÿ≠ÿ©\r\ndefinePageMeta({\r\n  layout: 'admin',\r\n  middleware: 'admin'\r\n})\r\n\r\n// ÿßŸÑŸÖÿ™ÿ∫Ÿäÿ±ÿßÿ™ ÿßŸÑÿ™ŸÅÿßÿπŸÑŸäÿ©\r\nconst pages = ref([])\r\nconst loading = ref(true)\r\nconst searchQuery = ref('')\r\nconst statusFilter = ref('')\r\nconst typeFilter = ref('')\r\nconst currentPage = ref(1)\r\nconst itemsPerPage = 10\r\n\r\n// ÿßŸÑŸÖÿ™ÿ∫Ÿäÿ±ÿßÿ™ ÿßŸÑŸÖÿ≠ÿ≥Ÿàÿ®ÿ©\r\nconst filteredPages = computed(() => {\r\n  let filtered = pages.value\r\n\r\n  // ÿ™ÿµŸÅŸäÿ© ÿßŸÑÿ®ÿ≠ÿ´\r\n  if (searchQuery.value) {\r\n    filtered = filtered.filter(page => \r\n      (page.title_ar && page.title_ar.toLowerCase().includes(searchQuery.value.toLowerCase())) ||\r\n      (page.title_en && page.title_en.toLowerCase().includes(searchQuery.value.toLowerCase())) ||\r\n      (page.title && page.title.toLowerCase().includes(searchQuery.value.toLowerCase())) ||\r\n      (page.content_ar && page.content_ar.toLowerCase().includes(searchQuery.value.toLowerCase())) ||\r\n      (page.content_en && page.content_en.toLowerCase().includes(searchQuery.value.toLowerCase())) ||\r\n      (page.content && page.content.toLowerCase().includes(searchQuery.value.toLowerCase()))\r\n    )\r\n  }\r\n\r\n  // ÿ™ÿµŸÅŸäÿ© ÿßŸÑÿ≠ÿßŸÑÿ©\r\n  if (statusFilter.value) {\r\n    filtered = filtered.filter(page => page.status === statusFilter.value)\r\n  }\r\n\r\n  // ÿ™ÿµŸÅŸäÿ© ÿßŸÑŸÜŸàÿπ\r\n  if (typeFilter.value) {\r\n    filtered = filtered.filter(page => page.type === typeFilter.value)\r\n  }\r\n\r\n  return filtered.slice((currentPage.value - 1) * itemsPerPage, currentPage.value * itemsPerPage)\r\n})\r\n\r\nconst totalItems = computed(() => {\r\n  let filtered = pages.value\r\n\r\n  if (searchQuery.value) {\r\n    filtered = filtered.filter(page => \r\n      (page.title_ar && page.title_ar.toLowerCase().includes(searchQuery.value.toLowerCase())) ||\r\n      (page.title_en && page.title_en.toLowerCase().includes(searchQuery.value.toLowerCase())) ||\r\n      (page.title && page.title.toLowerCase().includes(searchQuery.value.toLowerCase())) ||\r\n      (page.content_ar && page.content_ar.toLowerCase().includes(searchQuery.value.toLowerCase())) ||\r\n      (page.content_en && page.content_en.toLowerCase().includes(searchQuery.value.toLowerCase())) ||\r\n      (page.content && page.content.toLowerCase().includes(searchQuery.value.toLowerCase()))\r\n    )\r\n  }\r\n\r\n  if (statusFilter.value) {\r\n    filtered = filtered.filter(page => page.status === statusFilter.value)\r\n  }\r\n\r\n  if (typeFilter.value) {\r\n    filtered = filtered.filter(page => page.type === typeFilter.value)\r\n  }\r\n\r\n  return filtered.length\r\n})\r\n\r\nconst totalPages = computed(() => Math.ceil(totalItems.value / itemsPerPage))\r\n\r\n// ÿ•ÿ≠ÿµÿßÿ¶Ÿäÿßÿ™\r\nconst totalPagesCount = computed(() => pages.value.length)\r\nconst publishedPages = computed(() => pages.value.filter(page => page.status === 'published').length)\r\nconst draftPages = computed(() => pages.value.filter(page => page.status === 'draft').length)\r\nconst todayPages = computed(() => {\r\n  const today = new Date()\r\n  today.setHours(0, 0, 0, 0)\r\n  return pages.value.filter(page => new Date(page.created_at) >= today).length\r\n})\r\n\r\n// ÿ•ÿ≠ÿµÿßÿ¶Ÿäÿßÿ™ ÿßŸÑŸÖÿ≠ÿ™ŸàŸâ\r\nconst contentStats = computed(() => [\r\n  {\r\n    key: 'total',\r\n    label: 'ÿ•ÿ¨ŸÖÿßŸÑŸä ÿßŸÑÿµŸÅÿ≠ÿßÿ™',\r\n    value: totalPagesCount.value,\r\n    icon: 'material-symbols:article',\r\n    iconBg: 'bg-blue-100',\r\n    iconColor: 'text-blue-600'\r\n  },\r\n  {\r\n    key: 'published',\r\n    label: 'ŸÖŸÜÿ¥Ÿàÿ±',\r\n    value: publishedPages.value,\r\n    icon: 'material-symbols:visibility',\r\n    iconBg: 'bg-green-100',\r\n    iconColor: 'text-green-600'\r\n  },\r\n  {\r\n    key: 'draft',\r\n    label: 'ŸÖÿ≥ŸàÿØÿ©',\r\n    value: draftPages.value,\r\n    icon: 'material-symbols:edit',\r\n    iconBg: 'bg-orange-100',\r\n    iconColor: 'text-orange-600'\r\n  },\r\n  {\r\n    key: 'today',\r\n    label: 'ÿßŸÑŸäŸàŸÖ',\r\n    value: todayPages.value,\r\n    icon: 'material-symbols:today',\r\n    iconBg: 'bg-purple-100',\r\n    iconColor: 'text-purple-600'\r\n  }\r\n])\r\n\r\n// ÿ®ŸäÿßŸÜÿßÿ™ ÿßŸÑÿ±ÿ≥ŸàŸÖ ÿßŸÑÿ®ŸäÿßŸÜŸäÿ© ŸÑŸÑŸÖÿ≠ÿ™ŸàŸâ\r\nconst contentTypeData = computed(() => {\r\n  const typeCounts = pages.value.reduce((acc, page) => {\r\n    acc[page.type] = (acc[page.type] || 0) + 1\r\n    return acc\r\n  }, {})\r\n\r\n  return {\r\n    labels: Object.keys(typeCounts).map(type => getTypeName(type)),\r\n    datasets: [{\r\n      data: Object.values(typeCounts),\r\n      backgroundColor: [\r\n        'rgba(59, 130, 246, 0.8)',\r\n        'rgba(16, 185, 129, 0.8)',\r\n        'rgba(245, 158, 11, 0.8)',\r\n        'rgba(239, 68, 68, 0.8)'\r\n      ],\r\n      borderColor: [\r\n        'rgb(59, 130, 246)',\r\n        'rgb(16, 185, 129)',\r\n        'rgb(245, 158, 11)',\r\n        'rgb(239, 68, 68)'\r\n      ]\r\n    }]\r\n  }\r\n})\r\n\r\nconst contentStatusData = computed(() => {\r\n  const statusCounts = pages.value.reduce((acc, page) => {\r\n    acc[page.status] = (acc[page.status] || 0) + 1\r\n    return acc\r\n  }, {})\r\n\r\n  return {\r\n    labels: Object.keys(statusCounts).map(status => getStatusName(status)),\r\n    datasets: [{\r\n      data: Object.values(statusCounts),\r\n      backgroundColor: [\r\n        'rgba(34, 197, 94, 0.8)',\r\n        'rgba(245, 158, 11, 0.8)',\r\n        'rgba(107, 114, 128, 0.8)'\r\n      ],\r\n      borderColor: [\r\n        'rgb(34, 197, 94)',\r\n        'rgb(245, 158, 11)',\r\n        'rgb(107, 114, 128)'\r\n      ]\r\n    }]\r\n  }\r\n})\r\n\r\nconst monthlyPublishingData = computed(() => {\r\n  const monthlyCounts = {}\r\n  const months = ['ŸäŸÜÿßŸäÿ±', 'ŸÅÿ®ÿ±ÿßŸäÿ±', 'ŸÖÿßÿ±ÿ≥', 'ÿ£ÿ®ÿ±ŸäŸÑ', 'ŸÖÿßŸäŸà', 'ŸäŸàŸÜŸäŸà']\r\n  \r\n  // ÿ™ŸáŸäÿ¶ÿ© ÿßŸÑÿπÿØÿßÿØÿßÿ™\r\n  months.forEach(month => {\r\n    monthlyCounts[month] = 0\r\n  })\r\n\r\n  // ÿ≠ÿ≥ÿßÿ® ÿπÿØÿØ ÿßŸÑÿµŸÅÿ≠ÿßÿ™ ÿßŸÑŸÖŸÜÿ¥Ÿàÿ±ÿ© ŸÅŸä ŸÉŸÑ ÿ¥Ÿáÿ±\r\n  pages.value.forEach(page => {\r\n    if (page.status === 'published') {\r\n      const date = new Date(page.created_at)\r\n      const monthIndex = date.getMonth()\r\n      if (monthIndex < 6) { // ÿ¢ÿÆÿ± 6 ÿ£ÿ¥Ÿáÿ±\r\n        monthlyCounts[months[monthIndex]]++\r\n      }\r\n    }\r\n  })\r\n\r\n  return {\r\n    labels: months,\r\n    datasets: [{\r\n      label: 'ÿπÿØÿØ ÿßŸÑÿµŸÅÿ≠ÿßÿ™ ÿßŸÑŸÖŸÜÿ¥Ÿàÿ±ÿ©',\r\n      data: Object.values(monthlyCounts),\r\n      backgroundColor: 'rgba(139, 92, 246, 0.8)',\r\n      borderColor: 'rgb(139, 92, 246)',\r\n      borderWidth: 1\r\n    }]\r\n  }\r\n})\r\n\r\n// ÿ•ÿπÿØÿßÿØÿßÿ™ ÿßŸÑÿ±ÿ≥ŸàŸÖ ÿßŸÑÿ®ŸäÿßŸÜŸäÿ©\r\nconst contentTypeOptions = ref({\r\n  plugins: {\r\n    legend: {\r\n      position: 'bottom',\r\n      labels: {\r\n        padding: 15,\r\n        usePointStyle: true\r\n      }\r\n    }\r\n  }\r\n})\r\n\r\nconst contentStatusOptions = ref({\r\n  plugins: {\r\n    legend: {\r\n      position: 'bottom',\r\n      labels: {\r\n        padding: 15,\r\n        usePointStyle: true\r\n      }\r\n    }\r\n  }\r\n})\r\n\r\nconst monthlyPublishingOptions = ref({\r\n  plugins: {\r\n    legend: {\r\n      display: false\r\n    }\r\n  },\r\n  scales: {\r\n    y: {\r\n      beginAtZero: true,\r\n      ticks: {\r\n        stepSize: 1\r\n      }\r\n    }\r\n  }\r\n})\r\n\r\n// ÿ™ÿ≠ŸÖŸäŸÑ ÿßŸÑÿµŸÅÿ≠ÿßÿ™\r\nconst loadPages = async () => {\r\n  try {\r\n    loading.value = true\r\n    console.log('üîÑ Loading pages from API...')\r\n    const result = await $fetch('/api/content')\r\n    console.log('üìä API Response:', result)\r\n    \r\n    // Ensure pages.value is always an array\r\n    if (result && result.pages && Array.isArray(result.pages)) {\r\n      pages.value = result.pages\r\n      console.log('‚úÖ Loaded pages:', pages.value.length)\r\n    } else if (Array.isArray(result)) {\r\n      pages.value = result\r\n      console.log('‚úÖ Loaded pages (direct array):', pages.value.length)\r\n    } else {\r\n      pages.value = []\r\n      console.log('‚ö†Ô∏è No pages found, using empty array')\r\n    }\r\n  } catch (error) {\r\n    console.error('‚ùå ÿÆÿ∑ÿ£ ŸÅŸä ÿ™ÿ≠ŸÖŸäŸÑ ÿßŸÑÿµŸÅÿ≠ÿßÿ™:', error)\r\n    // ÿ®ŸäÿßŸÜÿßÿ™ ŸàŸáŸÖŸäÿ© ŸÑŸÑÿπÿ±ÿ∂\r\n    pages.value = [\r\n      {\r\n        id: 1,\r\n        title_ar: 'ŸÖŸÜ ŸÜÿ≠ŸÜ',\r\n        title_en: 'About Us',\r\n        content_ar: 'ŸÜÿ≠ŸÜ ŸàŸÉÿßŸÑÿ© ÿ≥ŸÅÿ± ÿ±ÿßÿ¶ÿØÿ© ŸÅŸä ÿßŸÑŸÖŸÖŸÑŸÉÿ© ÿßŸÑÿπÿ±ÿ®Ÿäÿ© ÿßŸÑÿ≥ÿπŸàÿØŸäÿ©',\r\n        content_en: 'We are a leading travel agency in Saudi Arabia',\r\n        type: 'page',\r\n        status: 'published',\r\n        created_at: new Date().toISOString()\r\n      },\r\n      {\r\n        id: 2,\r\n        title_ar: 'ÿ£ÿÆÿ®ÿßÿ± ÿßŸÑÿ≥ŸÅÿ±',\r\n        title_en: 'Travel News',\r\n        content_ar: 'ÿ£ÿ≠ÿØÿ´ ÿ£ÿÆÿ®ÿßÿ± ÿßŸÑÿ≥ŸÅÿ± ŸàÿßŸÑÿ≥Ÿäÿßÿ≠ÿ©',\r\n        content_en: 'Latest travel and tourism news',\r\n        type: 'news',\r\n        status: 'draft',\r\n        created_at: new Date(Date.now() - 86400000).toISOString()\r\n      }\r\n    ]\r\n    console.log('üìù Using fallback data:', pages.value.length, 'pages')\r\n  } finally {\r\n    loading.value = false\r\n  }\r\n}\r\n\r\n// ÿ™ÿ∫ŸäŸäÿ± ÿ≠ÿßŸÑÿ© ÿßŸÑÿµŸÅÿ≠ÿ©\r\nconst togglePageStatus = async (page) => {\r\n  try {\r\n    const newStatus = page.status === 'published' ? 'draft' : 'published'\r\n    \r\n    // TODO: Add API endpoint for updating page status\r\n    const index = pages.value.findIndex(p => p.id === page.id)\r\n    if (index !== -1) {\r\n      pages.value[index].status = newStatus\r\n    }\r\n\r\n    console.log(`ÿ™ŸÖ ${newStatus === 'published' ? 'ŸÜÿ¥ÿ±' : 'ÿ•ŸÑÿ∫ÿßÿ° ŸÜÿ¥ÿ±'} ÿßŸÑÿµŸÅÿ≠ÿ© ÿ®ŸÜÿ¨ÿßÿ≠`)\r\n  } catch (error) {\r\n    console.error('ÿÆÿ∑ÿ£ ŸÅŸä ÿ™ÿ∫ŸäŸäÿ± ÿ≠ÿßŸÑÿ© ÿßŸÑÿµŸÅÿ≠ÿ©:', error)\r\n  }\r\n}\r\n\r\n// ÿ≠ÿ∞ŸÅ ÿßŸÑÿµŸÅÿ≠ÿ©\r\nconst deletePage = async (page) => {\r\n  if (!confirm('ŸáŸÑ ÿ£ŸÜÿ™ ŸÖÿ™ÿ£ŸÉÿØ ŸÖŸÜ ÿ≠ÿ∞ŸÅ Ÿáÿ∞Ÿá ÿßŸÑÿµŸÅÿ≠ÿ©ÿü ŸÑÿß ŸäŸÖŸÉŸÜ ÿßŸÑÿ™ÿ±ÿßÿ¨ÿπ ÿπŸÜ Ÿáÿ∞ÿß ÿßŸÑÿ•ÿ¨ÿ±ÿßÿ°.')) return\r\n\r\n  try {\r\n    await $fetch(`/api/content/${page.id}`, { method: 'DELETE' })\r\n    \r\n    // Remove from local list\r\n    pages.value = pages.value.filter(p => p.id !== page.id)\r\n    \r\n    console.log('ÿ™ŸÖ ÿ≠ÿ∞ŸÅ ÿßŸÑÿµŸÅÿ≠ÿ© ÿ®ŸÜÿ¨ÿßÿ≠')\r\n  } catch (error) {\r\n    console.error('ÿÆÿ∑ÿ£ ŸÅŸä ÿ≠ÿ∞ŸÅ ÿßŸÑÿµŸÅÿ≠ÿ©:', error)\r\n  }\r\n}\r\n\r\n// ÿØŸàÿßŸÑ ÿßŸÑŸÖÿ≥ÿßÿπÿØÿ©\r\nconst formatDate = (dateString) => {\r\n  const date = new Date(dateString)\r\n  return date.toLocaleDateString('ar-SA')\r\n}\r\n\r\nconst truncateText = (text, length) => {\r\n  if (!text) return ''\r\n  return text.length > length ? text.substring(0, length) + '...' : text\r\n}\r\n\r\nconst getTypeName = (type) => {\r\n  const types = {\r\n    page: 'ÿµŸÅÿ≠ÿ©',\r\n    post: 'ŸÖŸÇÿßŸÑ',\r\n    news: 'ÿ£ÿÆÿ®ÿßÿ±'\r\n  }\r\n  return types[type] || type\r\n}\r\n\r\nconst getTypeColor = (type) => {\r\n  const colors = {\r\n    page: 'bg-blue-100 text-blue-800',\r\n    post: 'bg-green-100 text-green-800',\r\n    news: 'bg-purple-100 text-purple-800'\r\n  }\r\n  return colors[type] || 'bg-gray-100 text-gray-800'\r\n}\r\n\r\nconst getStatusName = (status) => {\r\n  const statuses = {\r\n    published: 'ŸÖŸÜÿ¥Ÿàÿ±',\r\n    draft: 'ŸÖÿ≥ŸàÿØÿ©',\r\n    archived: 'ŸÖÿ§ÿ±ÿ¥ŸÅ'\r\n  }\r\n  return statuses[status] || status\r\n}\r\n\r\nconst getStatusColor = (status) => {\r\n  const colors = {\r\n    published: 'bg-green-100 text-green-800',\r\n    draft: 'bg-orange-100 text-orange-800',\r\n    archived: 'bg-gray-100 text-gray-800'\r\n  }\r\n  return colors[status] || 'bg-gray-100 text-gray-800'\r\n}\r\n\r\n// ÿ•ÿπÿßÿØÿ© ÿ™ÿπŸäŸäŸÜ ÿßŸÑÿµŸÅÿ≠ÿ© ÿπŸÜÿØ ÿ™ÿ∫ŸäŸäÿ± ÿßŸÑŸÅŸÑÿßÿ™ÿ±\r\nwatch([searchQuery, statusFilter, typeFilter], () => {\r\n  currentPage.value = 1\r\n})\r\n\r\n// ÿ™ÿ≠ŸÖŸäŸÑ ÿßŸÑÿ®ŸäÿßŸÜÿßÿ™ ÿπŸÜÿØ ÿ™ÿ≠ŸÖŸäŸÑ ÿßŸÑÿµŸÅÿ≠ÿ©\r\nonMounted(async () => {\r\n  // ÿßŸÑÿ™ÿ£ŸÉÿØ ŸÖŸÜ ÿ£ŸÜ ÿßŸÑŸÖÿ≥ÿ™ÿÆÿØŸÖ ŸÖÿµÿßÿØŸÇ ÿπŸÑŸäŸá ŸÇÿ®ŸÑ ÿ™ÿ≠ŸÖŸäŸÑ ÿßŸÑÿ®ŸäÿßŸÜÿßÿ™\r\n  const { checkAuth } = useAuth()\r\n  try {\r\n    await checkAuth(true)\r\n    await loadPages()\r\n  } catch (error) {\r\n    console.error('ÿÆÿ∑ÿ£ ŸÅŸä ÿ™ÿ≠ŸÖŸäŸÑ ÿ®ŸäÿßŸÜÿßÿ™ ÿßŸÑŸÖÿ≠ÿ™ŸàŸâ:', error)\r\n  }\r\n})\r\n\r\n// SEO ŸàÿßŸÑŸÖŸäÿ™ÿß\r\nuseHead({\r\n  title: 'ÿ•ÿØÿßÿ±ÿ© ÿßŸÑŸÖÿ≠ÿ™ŸàŸâ - Wonder Land Admin',\r\n  meta: [\r\n    { name: 'description', content: 'ÿ•ÿØÿßÿ±ÿ© ÿµŸÅÿ≠ÿßÿ™ ÿßŸÑŸÖŸàŸÇÿπ ŸàÿßŸÑŸÖÿ≠ÿ™ŸàŸâ' },\r\n    { name: 'robots', content: 'noindex, nofollow' }\r\n  ]\r\n})\r\n</script>\r\n\r\n"],"version":3}